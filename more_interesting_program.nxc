/*
Десять квадратов.
По алгоритму данной программы робот едет по траектории квадратной формы 10 раз
*/
#define MOVE_TIME 500 // Время движения вперёд
#define TURN_TIME 360 // Время, в течение которого робот поворачивает
task main()
{
  repeat(10) // Объявляем цикл, повторяющий действия 10 раз
  {
    repeat(4)
    {
      OnFwd(OUT_AC, 75);
      Wait(MOVE_TIME);
      OnRev(OUT_C, 75);
      Wait(TURN_TIME);
    }
  }
  Off(OUT_AC); // Выключаем моторы
}
